<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="174.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainFormController">
   <children>
      <Label layoutX="23.0" layoutY="13.0" text="Select Your Gender" textFill="#05b531">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Separator layoutX="11.0" layoutY="53.0" prefHeight="4.0" prefWidth="580.0" />
      <RadioButton fx:id="rBtnMale" layoutX="20.0" layoutY="75.0" mnemonicParsing="false" selected="true" text="Male" textFill="#8c0de8">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rBtnFemale" layoutX="115.0" layoutY="76.0" mnemonicParsing="false" text="Female" textFill="#8c0de8" toggleGroup="$gender">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </RadioButton>
      <Button layoutX="21.0" layoutY="117.0" mnemonicParsing="false" onAction="#printDataOnAction" text="Print Data" textFill="#bf1010">
         <font>
            <Font name="Calibri" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
